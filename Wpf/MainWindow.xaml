<Window x:Class="Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Margin="20">
        <Button Click="Button_Click">Canvas</Button>
        <Button Click="Button_Click_1">WrapPanel</Button>
        <Button Click="Button_Click_2">StackPanel</Button>
        <Button Click="Button_Click_3">DockPanel</Button>
        <Button Click="Button_Click_4">Grid</Button>
        <Button Click="Button_Click_5">Grid 2</Button>
        <Button Click="Button_Click_6">Grid 3</Button>
        <Button Click="Button_Click_7">Grid 4</Button>
        <Button Click="Button_Click_8">Grid 5</Button>
        <Button Click="Button_Click_9">Grid 6</Button>
    </StackPanel>
</Window>
